{
  "manifest_version": 3, 
  "name": "Oslyn",
  "version": "0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
        "matches": ["https://tabs.ultimate-guitar.com/tab/*"],
        "js": ["contentScript.js"]
    }
  ]
}